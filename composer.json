{
  "name": "potherca/phpcodesniffer-composer-installer",
  "description": "⚠️ Please migrate to phpcsstandards/composer-installer ⚠️",
  "type": "composer-plugin",
  "abandoned": true,
  "suggest": {
    "phpcsstandards/composer-installer": "The renamed version of this package"
  },
  "homepage": "https://github.com/PHPCSStandards/composer-installer",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/PHPCSStandards/composer-installer/issues",
    "source": "https://github.com/PHPCSStandards/composer-installer"
  },
  "require": {
    "phpcsstandards/composer-installer": "*"
  },
  "autoload": {
    "psr-4": {
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\": "src/"
    }
  },
  "extra": {
    "class": "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin"
  },
  "scripts": {
    "install-codestandards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ]
  }
}
